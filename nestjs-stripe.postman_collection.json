              ]
            },
            "description": "Create a new order (requires authentication)"
          }
        }
      ]
    },
    {
      "name": "Payments",
      "item": [
        {
          "name": "Get Available Payment Methods",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "description": "JWT token required for authenticated access"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/payments/methods",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "payments",
                "methods"
              ]
            },
            "description": "Get list of available payment methods (stripe, paypal)"
          }
        },
        {
          "name": "Create Payment Intent (Authenticated)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "description": "JWT token required for authenticated access"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"orderId\": \"17c82430-92ce-4c6a-a9d8-00d6f22d0b34\",\n  \"paymentMethod\": \"stripe\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/payments/create-intent",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "payments",
                "create-intent"
              ]
            },
            "description": "Create payment intent with specified payment method"
          }
        },
        {
          "name": "Create Checkout Session (Authenticated)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "description": "JWT token required for authenticated access"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"orderId\": \"17c82430-92ce-4c6a-a9d8-00d6f22d0b34\",\n  \"paymentMethod\": \"paypal\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/payments/create-checkout-session",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "payments",
                "create-checkout-session"
              ]
            },
            "description": "Create checkout session with specified payment method"
          }
        },
        {
          "name": "Request Refund (ADMIN)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "description": "JWT token required for admin access"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"orderId\": \"17c82430-92ce-4c6a-a9d8-00d6f22d0b34\",\n  \"reason\": \"Customer requested refund\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/payments/request-refund",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "payments",
                "request-refund"
              ]
            }
          }
        },
        {
          "name": "Payment Success (Authenticated)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "description": "JWT token required for authenticated access"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/payments/success?session_id=cs_test_1234567890abcdef",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "payments",
                "success"
              ],
              "query": [
                {
                  "key": "session_id",
                  "value": "cs_test_1234567890abcdef",
                  "description": "Stripe checkout session ID"
                }
              ]
            },
            "description": "Handle successful payment redirect"
          }
        },
        {
          "name": "Payment Cancel (Authenticated)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "description": "JWT token required for authenticated access"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/payments/cancel",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "payments",
                "cancel"
              ]
            },
            "description": "Handle cancelled payment redirect"
          }
        },
        {
          "name": "PayPal Payment Success (Authenticated)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "description": "JWT token required for authenticated access"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/payments/paypal/success?token=EC-1234567890&orderId=17c82430-92ce-4c6a-a9d8-00d6f22d0b34&PayerID=PAYER123",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "payments",
                "paypal",
                "success"
              ],
              "query": [
                {
                  "key": "token",
                  "value": "EC-1234567890",
                  "description": "PayPal token"
                },
                {
                  "key": "orderId",
                  "value": "17c82430-92ce-4c6a-a9d8-00d6f22d0b34",
                  "description": "Order ID"
                },
                {
                  "key": "PayerID",
                  "value": "PAYER123",
                  "description": "PayPal Payer ID"
                }
              ]
            },
            "description": "Handle successful PayPal payment redirect"
          }
        },
        {
          "name": "PayPal Payment Cancel (Authenticated)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "description": "JWT token required for authenticated access"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/payments/paypal/cancel",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "payments",
                "paypal",
                "cancel"
              ]
            },
            "description": "Handle cancelled PayPal payment redirect"
          }
        },
        {
          "name": "Stripe Webhook",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "stripe-signature",
                "value": "t=1234567890,v1=signature_here",
                "description": "Stripe webhook signature for verification"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"id\": \"evt_test_webhook\",\n  \"object\": \"event\",\n  \"type\": \"payment_intent.succeeded\",\n  \"data\": {\n    \"object\": {\n      \"id\": \"pi_test_123\",\n      \"amount\": 2999\n    }\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/payments/webhook/stripe",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "payments",
                "webhook",
                "stripe"
              ]
            },
            "description": "Stripe webhook endpoint for payment events"
          }
        },
        {
          "name": "PayPal Webhook",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "paypal-signature",
                "value": "signature_here",
                "description": "PayPal webhook signature for verification"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"id\": \"WH-1234567890\",\n  \"event_type\": \"PAYMENT.CAPTURE.COMPLETED\",\n  \"resource\": {\n    \"id\": \"capture_id_here\",\n    \"amount\": {\n      \"value\": \"29.99\"\n    }\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/payments/webhook/paypal",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "payments",
                "webhook",
                "paypal"
              ]
            },
            "description": "PayPal webhook endpoint for payment events"
          }
        }
